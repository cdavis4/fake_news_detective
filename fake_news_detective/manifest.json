{
  "name": "Fake News Detective",
  "version": "1.0",
  "description": "Allows users to determine the validity of their news sources.",
  "permissions": [
    "activeTab", 
    "declarativeContent", 
    "storage",
    "geolocation",
    "identity",
    "pageCapture",
    "tabs",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
 
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/base16.png",
      "32": "images/base32.png",
      "48": "images/base48.png",
      "128": "images/base128.png"
    }
  },
  "icons": {
    "16": "images/base16.png",
    "32": "images/base32.png",
    "48": "images/base48.png",
    "128": "images/base128.png"
  },
  "oauth2": {
    "client_id": "657083043856-7dlffqtji03v3k2201fekgh0r8caj42m.apps.googleusercontent.com",
    "scopes":[
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "key": "",
  "manifest_version": 2
}
